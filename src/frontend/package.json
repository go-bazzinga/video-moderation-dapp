		"@dfinity/candid": "0.10.0",
		"@dfinity/identity": "0.10.0",
		"@dfinity/principal": "0.10.0",
		"firebase": "^9.8.2",
		"svelte-local-storage-store": "^0.2.6"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "next",
		"@sveltejs/kit": "next",
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"@typescript-eslint/parser": "^5.10.1",
		"autoprefixer": "^10.4.7",
		"buffer": "6.0.3",
		"eslint": "^8.12.0",
